sudo ln -s /home/$USER/sh/cache /usr/local/bin/cache
sudo ln -s /home/$USER/sh/go /usr/local/bin/go
sudo ln -s /home/$USER/sh/bf /usr/local/bin/bf
sudo ln -s /home/$USER/sh/tests /usr/local/bin/tests

chmod +x /home/$USER/sh/cache
chmod +x /home/$USER/sh/go
chmod +x /home/$USER/sh/bf
chmod +x /home/$USER/sh/tests
